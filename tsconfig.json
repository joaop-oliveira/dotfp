{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@velit/dotfp": [
        "libs/dotfp/src"
      ],
      "@velit/dotfp/*": [
        "libs/dotfp/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}